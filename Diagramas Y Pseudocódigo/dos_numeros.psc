Algoritmo dos_numeros
	//Definición de variables
	Definir v1,v2 Como Real;
	//Datos de entrada
	Escribir 'Introduce dos valores numéricos:';
	Leer v1,v2;
	//Proceso
	suma<-v1+v2;
	resta<-v1-v2;
	multiplicacion<-v1*v2;
	division<-v1/v2;
	//Salida
	Escribir 'Suma: ',suma;
	Escribir 'Resta: ',resta;
	Escribir 'Multiplicación: ',multiplicacion;
	Escribir 'División: ',division;
	
FinAlgoritmo

